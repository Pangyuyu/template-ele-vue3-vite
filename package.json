{
  "name": "guxing-electron-template",
  "id": "guxing-electron-template",
  "version": "0.0.3",
  "description": "electron+ts+vue3+vite模板",
  "private": false,
  "author": "guxing",
  "email": "813102522@qq.com",
  "url": "https://gitee.com/pangyu_yanyan/template-ele-vue3-vite/issues",
  "homepage": "https://pangyu_yanyan.gitee.io/know/",
  "main": "electron/main.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/pangyu_yanyan/template-ele-vue3-vite.git"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "serve": "vite preview",
    "electron": "yarn run loadhint && wait-on tcp:3000 && cross-env IS_DEV=true electron . --trace-warnings ...",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn run dev\" \"yarn run electron\"",
    "electron:build.win": "yarn run build && electron-builder --win --dir",
    "electron:build.linux": "yarn run build && electron-builder --linux",
    "electron:build.linux.appImage": "yarn run build && electron-builder --linux appImage",
    "electron:build.linux.deb": "yarn run build && electron-builder --linux deb",
    "electron:build.test": "yarn run build && electron-builder --dir",
    "electron:build.exe_all": "yarn run build && electron-builder --win --x64 --ia32",
    "electron:build.exe_x64": "yarn run build && electron-builder --win --x64",
    "electron:build.exe_x86": "yarn run build && electron-builder --win --ia32",
    "loadhint": "bash ./loadhint.bash"
  },
  "packageManager": "yarn@1.22.18",
  "build": {
    "appId": "com.guxing.template",
    "productName": "孤星Electron示例",
    "artifactName": "${productName}_${version}_${os}_${arch}.${ext}",
    "copyright": "Copyright © 2022 ${author}",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./src/assets/icons/logo.png"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "./src/assets/icons/logo.png"
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "./build/icons"
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    },
    "extraResources": {
      "from": "./resources",
      "to": "./"
    }
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@element-plus/icons-vue": "1.1.4",
    "@types/spark-md5": "^3.0.2",
    "axios": "0.27.2",
    "consola": "^2.15.3",
    "default-passive-events": "^2.0.0",
    "element-plus": "2.1.11",
    "ffi-napi": "^4.0.3",
    "querystring": "^0.2.1",
    "spark-md5": "^3.0.2",
    "url": "^0.11.0",
    "vue": "3.2.33",
    "vue-router": "4.0.14",
    "xlsx": "0.18.3"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@vitejs/plugin-vue": "2.2.4",
    "@vue/compiler-sfc": "3.2.33",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "18.2.0",
    "electron-builder": "^22.14.13",
    "pinia": "^2.0.13",
    "sass": "^1.35.1",
    "sass-loader": "12.3.0",
    "script-loader": "^0.7.2",
    "scss": "^0.2.4",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "0.5.11",
    "unplugin-vue-components": "^0.17.18",
    "vite": "2.9.8",
    "vite-plugin-components": "0.13.3",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "1.4.1",
    "vue-tsc": "^0.34.11",
    "wait-on": "^6.0.1"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@element-plus/icons-vue",
        "axios",
        "default-passive-events",
        "element-plus",
        "element-plus/es",
        "element-plus/es/components/button/style/css",
        "element-plus/es/components/card/style/css",
        "element-plus/es/components/checkbox-group/style/css",
        "element-plus/es/components/checkbox/style/css",
        "element-plus/es/components/config-provider/style/css",
        "element-plus/es/components/date-picker/style/css",
        "element-plus/es/components/dialog/style/css",
        "element-plus/es/components/divider/style/css",
        "element-plus/es/components/form-item/style/css",
        "element-plus/es/components/form/style/css",
        "element-plus/es/components/icon/style/css",
        "element-plus/es/components/image/style/css",
        "element-plus/es/components/input/style/css",
        "element-plus/es/components/menu-item/style/css",
        "element-plus/es/components/menu/style/css",
        "element-plus/es/components/option/style/css",
        "element-plus/es/components/pagination/style/css",
        "element-plus/es/components/popover/style/css",
        "element-plus/es/components/radio-group/style/css",
        "element-plus/es/components/radio/style/css",
        "element-plus/es/components/select/style/css",
        "element-plus/es/components/tab-pane/style/css",
        "element-plus/es/components/table-column/style/css",
        "element-plus/es/components/table/style/css",
        "element-plus/es/components/tabs/style/css",
        "element-plus/es/components/upload/style/css",
        "element-plus/lib/locale/lang/zh-cn",
        "querystring",
        "vue",
        "vue-router",
        "xlsx"
      ]
    }
  }
}
